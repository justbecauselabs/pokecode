{
  "expo": {
    "name": "Pok√©code",
    "slug": "pokecode",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#282c34"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.justbecauselabs.pokecode",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#282c34"
      },
      "package": "com.justbecauselabs.pokecode"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#282c34"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "pokecode",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "251a23a9-2a85-46c1-b348-f50e3f654a2a"
      }
    }
  }
}
